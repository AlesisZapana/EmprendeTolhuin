<form method="post" action="{{ path('estadosolicitud_delete', {'id': estadosolicitud.id}) }}" onsubmit="return confirm('Seguro que deseas eliminar este estado?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ estadosolicitud.id) }}">
    <button class="btn btn-danger btn-sm">Eliminar</button>
</form>
